{
  "name": "ramen",
  "version": "0.3.0",
  "description": "A simple timer for ramen",
  "homepage": "https://sprout2000.github.io/ramen",
  "private": true,
  "license": "MIT",
  "author": {
    "name": "sprout2000"
  },
  "scripts": {
    "start": "run-p watch:*",
    "watch:wait": "wait-on public/main.js && reload -d public -b",
    "watch:build": "NODE_ENV=\\\"development\\\" ts-node ./esbuild.ts",
    "build": "NODE_ENV=\\\"production\\\" ts-node ./esbuild.ts",
    "clean": "rm -f public/*.{js,css,map,mp3,png,woff*}"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "howler": "^2.2.1",
    "nosleep.js": "^0.12.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "typeface-roboto-mono": "1.1.13"
  },
  "devDependencies": {
    "@types/howler": "^2.1.2",
    "@types/node": "^14.14.25",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/workbox-build": "^5.0.0",
    "esbuild": "^0.8.43",
    "npm-run-all": "^4.1.5",
    "reload": "^3.1.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5",
    "wait-on": "^5.2.1",
    "workbox-build": "^6.1.0"
  },
  "optionalDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.15.0",
